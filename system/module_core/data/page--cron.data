pages
- cron|page
    title: Cron
    display
      check: url
      where: path
      match: %^/cron/(?<key>[a-z0-9]{40})$%
    access
      roles
      - =: anonymous
      - =: registered
    children
    - block_cron|page_part
        region: content
        type: code
        source: \effcore\modules\core\events_page_cron::on_cron_run
entities
- tree|entity
    name: tree
    title: Tree
    title_plural: Trees
    catalog_name: trees
    managing_is_enabled: true
    managing_group_id: menu
    has_parallel_checking: true
    ws_is_embedded: true
    ws_module_id: true
    ws_data: true
    access_select
      roles
      - =: admins
      permissions
      - =: manage_data__menu_tree__select
    access_insert
      roles
      - =: admins
      permissions
      - =: manage_data__menu_tree__insert
    access_update
      roles
      - =: admins
      permissions
      - =: manage_data__menu_tree__update
    access_delete
      roles
      - =: admins
      permissions
      - =: manage_data__menu_tree__delete
    fields
    - id
        title: ID
        type: varchar
        size: 255
        collate: nocase
        not_null: true
        check: (id <> '')
        managing_on_select_multiple_is_enabled: true
        managing_on_select_is_enabled: true
        managing_on_insert_is_enabled: true
        managing_on_update_is_enabled: true
        managing_control_class: \effcore\field_id_text
        managing_control_properties
        - is_validate_uniqueness: true
        - weight: +400
        managing_control_element_attributes_on_update
        - disabled: true
        selection_params_default
        - weight: +400
    - title
        title: Title
        type: varchar
        size: 255
        collate: nocase
        not_null: true
        check: (title <> '')
        filter_insert: \effcore\core::return_htmlspecialchars_encoded
        filter_update: \effcore\core::return_htmlspecialchars_encoded
        managing_on_select_multiple_is_enabled: true
        managing_on_select_is_enabled: true
        managing_on_insert_is_enabled: true
        managing_on_update_is_enabled: true
        managing_control_class: \effcore\field_text
        managing_control_properties
        - weight: +380
        selection_params_default
        - filter: \effcore\translation::apply
        - weight: +380
    - title_is_visible
        title: Title is visible
        type: boolean
        not_null: true
        default: 1
        managing_on_select_is_enabled: true
        managing_on_insert_is_enabled: true
        managing_on_update_is_enabled: true
        managing_control_class: \effcore\field_logic
        managing_control_properties
        - weight: +360
        managing_control_properties_on_insert
        - selected
          - =: 1
        selection_params_default
        - filter: \effcore\translation::apply
        - weight: +360
    - attributes
        title: Attributes
        type: blob
        filter_select: unserialize
        filter_insert: \effcore\core::data_serialize
        filter_update: \effcore\core::data_serialize
        managing_on_select_is_enabled: true
        managing_on_insert_is_enabled: true
        managing_on_update_is_enabled: true
        managing_control_class: \effcore\widget_attributes
        managing_control_properties
        - name_complex: attributes
        - weight: +340
        selection_params_default
        - filter: \effcore\widget_attributes::complex_value_to_markup
        - weight: +340
    - created
        title: Created
        type: datetime
        default: null
        check: (created <> '')
        managing_on_select_is_enabled: true
        managing_control_class: \effcore\field_datetime_local
        selection_params_default
        - weight: +320
    - updated
        title: Updated
        type: datetime
        default: null
        check: (updated <> '')
        managing_on_select_is_enabled: true
        managing_control_class: \effcore\field_datetime_local
        selection_params_default
        - weight: +300
    - access
        title: Read access
        type: blob
        filter_select: unserialize
        filter_insert: \effcore\core::data_serialize
        filter_update: \effcore\core::data_serialize
        managing_on_insert_is_enabled: true
        managing_on_update_is_enabled: true
        managing_control_class: \effcore\widget_access
        selection_params_default
        - weight: +280
    constraints
    - primary_id
        type: primary
        fields
        - =: id
    indexes
    - index_title
        type: index
        fields
        - =: title
    - index_created
        type: index
        fields
        - =: created
    - index_updated
        type: index
        fields
        - =: updated

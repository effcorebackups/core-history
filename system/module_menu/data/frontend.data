frontend
- tree_all|frontend
    display
      check: block
      where: dpath
      match: %^trees/.*$%
    styles
    - tree_style
        path: frontend/tree.cssd
        attributes
        - rel: stylesheet
        - media: all
    scripts
    - tree_script
        path: frontend/tree.js
        attributes
        - defer: true
- tabs_all|frontend
    display
      check: block
      where: dpath
      match: %^tabs/.*$%
    styles
    - tabs_style
        path: frontend/tabs.cssd
        attributes
        - rel: stylesheet
        - media: all
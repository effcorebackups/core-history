frontend
- menu_block_tree
    display
      check: block
      where: dpath
      match: %^trees/.*$%
    styles
    - tree_style
        file: frontend/tree.cssd
        media: all
- menu_block_tabs
    display
      check: block
      where: dpath
      match: %^tabs/.*$%
    styles
    - tabs_style
        file: frontend/tabs.cssd
        media: all
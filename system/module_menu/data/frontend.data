frontend
- menu_all
    display
      check: block
      where: dpath
      match: %^trees/.*$%
    styles
    - tree_style
        file: frontend/tree.cssd
        attributes
        - rel: stylesheet
        - media: all
- tabs_all
    display
      check: block
      where: dpath
      match: %^tabs/.*$%
    styles
    - tabs_style
        file: frontend/tabs.cssd
        attributes
        - rel: stylesheet
        - media: all
    fields
    - id
        title: ID
        type: autoincrement
        not_null: true
    - id_parent
        title: Parent ID
        type: integer
        not_null: true
    - title
        title: Title
        type: varchar
        size: 255
        not_null: true
    constraints
    - key_id
        type: primary key
        fields
        - =: id
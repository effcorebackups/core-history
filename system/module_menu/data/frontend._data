frontend
- frontend_block_tree
    display
      check: block
      where: dpath
      match: %^trees/.*$%
    styles
    - tree_style
        file: frontend/tree._style
        media: all
- frontend_block_tabs
    display
      check: block
      where: dpath
      match: %^tabs/.*$%
    styles
    - tabs_style
        file: frontend/tabs._style
        media: all
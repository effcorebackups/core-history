tab_items
- data_menu|Tab_item
    id: data_menu
    id_parent: null
    id_tab: data
    action_name: menu
    action_name_default: menu/tree
    title: Menu
    weight: +480
    access
      roles
      - =: admins
      permissions_match
      - =: %^manage_data__menu_.+__select$%
- data_menu_tree|Tab_item
    id: data_menu_tree
    id_parent: data_menu
    id_tab: data
    action_name: menu/tree
    title: Trees
    weight: +500
    access
      roles
      - =: admins
      permissions
      - =: manage_data__menu_tree__select
- data_menu_tree_item|Tab_item
    id: data_menu_tree_item
    id_parent: data_menu
    id_tab: data
    action_name: menu/tree_item
    title: Tree items
    weight: +480
    access
      roles
      - =: admins
      permissions
      - =: manage_data__menu_tree_item__select
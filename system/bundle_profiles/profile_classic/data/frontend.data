frontend
- page_all_profile_classic|frontend
    display
      check: url
      where: path
      match: %^.*$%
    styles
    - page_style
        path: frontend/page.cssd?id_page=%%_id_page_context
        attributes
        - rel: stylesheet
        - media: all
        weight: -50
    favicons
  # icon classic for browsers: Safari, Opera, Chrome, IE 9-11, Edge …
    - icon_32_classic
        path: frontend/images/icon-favourite-32.ico
        attributes
        - rel: icon
        - type: image/x-icon
        - sizes: 32x32
  # icon for all modern browsers: Safari, Opera, Chrome, Firefox, IE 11, Edge …
    - icon_32_modern
        path: frontend/images/icon-favourite-32.png
        attributes
        - rel: icon
        - type: image/png
        - sizes: 32x32
  # icon for Safari on iPad 1/2
    - icon_apple_76
        path: frontend/images/icon-favourite-76.png
        attributes
        - rel: apple-touch-icon
        - type: image/png
        - sizes: 76x76
  # icon for Safari on iPhone
    - icon_apple_120
        path: frontend/images/icon-favourite-120.png
        attributes
        - rel: apple-touch-icon
        - type: image/png
        - sizes: 120x120
  # icon for Safari on any iPad
    - icon_apple_152
        path: frontend/images/icon-favourite-152.png
        attributes
        - rel: apple-touch-icon
        - type: image/png
        - sizes: 152x152
  # icon for Safari on iPad 3+, iPhone, macOS
    - icon_apple_192
        path: frontend/images/icon-favourite-192.png
        attributes
        - rel: apple-touch-icon
        - type: image/png
        - sizes: 192x192
- page_about|instance
    entity_name: page
    values
    - id: about
    - id_layout: original_classic
    - title: About Us
    - url: /about
    - is_https: 0
    - module_id: profile_classic
    - meta: <meta name="description" content="">
////////////<meta name="keywords" content="">
    - blocks
      - system
        - block__page_actions|block_preset_link
            id: block__page_actions
        - block__tree_sql__user_registered|block_preset_link
            id: block__tree_sql__user_registered
            title: Registered user's menu
            attributes
            - data-block: true
            - data-id: block__tree_sql__user_registered
            - data-at-right: true
        - block__tree_sql__user_anonymous|block_preset_link
            id: block__tree_sql__user_anonymous
            title: Anonymous user's menu
            attributes
            - data-block: true
            - data-id: block__tree_sql__user_anonymous
            - data-at-right: true
        - block__tree__system|block_preset_link
            id: block__tree__system
      - header
        - block__picture_sql__logotype_classic|block_preset_link
            id: block__picture_sql__logotype_classic
            title: Logotype
      - menu
        - block__tree_sql__main_classic|block_preset_link
            id: block__tree_sql__main_classic
            title: Main menu
      - messages
        - block__messages|block_preset_link
            id: block__messages
      - title
        - block__title|block_preset_link
            id: block__title
      - content
        - block__text_sql__about_classic|block_preset_link
            id: block__text_sql__about_classic
            title: The main text of the page
      - footer
        - block__text_sql__copyright_classic|block_preset_link
            id: block__text_sql__copyright_classic
            title: Copyright
            attributes
            - data-block: true
            - data-id: block__text_sql__copyright_classic
            - data-at-left: true

selections
- logotype_classic_main|selection
    id: logotype_classic_main
    title: Logotype Main
    template: children
    decorator_params
    - view_type: template
    - template: children
    - template_row: logotype
    - template_row_mapping
      - title: logotype.title
      - url: logotype.url
      - src: logotype.path
    query_params
    - conditions
      - id_!f: ~logotype.id
      - operator: =
      - id_!v: classic_main
    - limit: 1
    fields
    - logotype.title
        type: field
        entity_name: logotype
        entity_field_name: title
        is_apply_translation: true
    - logotype.url
        type: field
        entity_name: logotype
        entity_field_name: url
    - logotype.path
        type: field
        entity_name: logotype
        entity_field_name: path
- text_classic_copyright|selection
    id: text_classic_copyright
    title: Text Copyright
    template: children
    decorator_params
    - view_type: template
    - template: children
    - template_row: text
    - template_row_mapping
      - text: text.text
    query_params
    - conditions
      - id_!f: ~text.id
      - operator: =
      - id_!v: classic_copyright
    - limit: 1
    fields
    - text.text
        type: field
        entity_name: text
        entity_field_name: text
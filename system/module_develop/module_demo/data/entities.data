        not_null: true
- demo_indexes|entity
    name: demo_indexes
    title: Demo: indexes
    title_plural: Demo: indexes
    catalog_name: demo_indexes
    fields
    - f_integer_1
        title: Field integer 1
        type: integer
        not_null: true
        default: 0
        show_in_manager: true
    - f_integer_2
        title: Field integer 2
        type: integer
        not_null: true
        default: 0
        show_in_manager: true
    indexes
    # ─────────────────────────────────────────────────────────────────────
    # CREATE UNIQUE INDEX name ON table_name (field_1, …);
    # ─────────────────────────────────────────────────────────────────────
    - index_f_integer_1
        type: unique index
        fields
        - =: f_integer_1
    # ─────────────────────────────────────────────────────────────────────
    # CREATE INDEX name ON table_name (field_1, …);
    # ─────────────────────────────────────────────────────────────────────
    - index_f_integer_2
        type: index
        fields
        - =: f_integer_2
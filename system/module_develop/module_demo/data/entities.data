- demo_indexes|entity
    name: demo_indexes
    title: Indexes
    title_plural: Indexes
    catalog_name: demo_indexes
    group: Demo
    is_managed: true
    fields
    - id
        title: ID
        type: autoincrement
        not_null: true
        field_is_visible_on_select: true
    - f_index_unique
        title: Field index unique
        type: varchar
        size: 255
        collate: nocase
        not_null: true
        field_is_visible_on_select: true
    - f_index
        title: Field index
        type: varchar
        size: 255
        collate: nocase
        not_null: true
        field_is_visible_on_select: true
    constraints
    - primary_id
        type: primary
        fields

- demo_join|selection
    id: demo_join
    title: Selection of "demo_data" entities + join
    pager_is_enabled: true
    decorator_params
    - view_type: table-adaptive
    query_params
    - join_script
      - with_demo_join
          type: left outer join
          entity_name: demo_join
          entity_field_name: id_data
          on_entity_name: demo_data
          on_entity_field_name: id
    - conditions
      - id_!f: ~demo_data.id
      - id_operator: >=
      - id_!v: 100
    - order
      - title_!f: ~demo_data.id
      - type: ASC
    - limit: 5
    fields
    - demo_data.id
        type: field
        entity_name: demo_data
        entity_field_name: id
    - demo_data.nickname
        type: field
        entity_name: demo_data
        entity_field_name: nickname
    - demo_data.created
        type: field
        entity_name: demo_data
        entity_field_name: created
    - demo_data.is_even
        type: field
        entity_name: demo_data
        entity_field_name: is_even
        filter: \effcore\translation::apply
    - demo_join.email
        type: join_field
        entity_name: demo_join
        entity_field_name: email

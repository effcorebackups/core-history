selections
- demo_join|selection
    id: demo_join
    title: Selection with JOIN
    view_type: ul
    join
    - relation_role_ws_user
        entity_name: relation_role_ws_user
        field_name: id_user
        on_entity_name: user
        on_field_name: id
    - role
        entity_name: role
        field_name: id
        on_entity_name: relation_role_ws_user
        on_field_name: id_role
    query_params
    - conditions
      - id_!f: ~user.id
      - operator: =
      - id_!v: 1
    - order
      - title_!f: ~role.title
      - type: ASC
    - limit: 2
    fields
    - user.nick
        type: field
        entity_name: user
        field_name: nick
    - user.created
        type: field
        entity_name: user
        field_name: created
    - user.updated
        type: field
        entity_name: user
        field_name: updated
    - role.title
        type: join_field
        entity_name: role
        field_name: title
    - relation_role_ws_user.id_role
        type: join_field
        entity_name: relation_role_ws_user
        field_name: id_role

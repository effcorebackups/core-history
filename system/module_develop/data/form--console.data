forms
- console|form
    attributes
    - id: console
    - method: post
    - novalidate: true
    children
    - settings|fieldset
        title: Settings
        children
        - visibility|field_select
            title: Visibility
            description: *.cssd | *.jsd console will show only in mode "show for everyone" for performance reasons.
            description_state: opened
            element_attributes
            - name: visibility
            values
            - not_selected: - select -
            - not_show: not show
            - show_for_admin: show for admin
            - show_for_everyone: show for everyone
            selected
            - =: not_show
    - button_save|button
        title: save
        attributes
        - value: save
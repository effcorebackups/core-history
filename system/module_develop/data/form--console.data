forms
- console|form
    attributes
    - id: console
    - method: post
    - novalidate: true
    children
    - settings|fieldset
        title: Settings
        children
        - visibility|field_select
            title: Visibility
            description: For performance reasons, the console in the file types "*.cssd" and "*.jsd" will only be displayed in the "show for everyone" mode.
            description_state: opened
            element_attributes
            - name: visibility
            values
            - not_selected: - select -
            - not_show: do not show
            - show_for_admin: show for admin
            - show_for_everyone: show for everyone
            selected
            - =: not_show
    - button_save|button
        title: save
        attributes
        - value: save
frontend
- frontend_develop_page_n
    display
      check: url
      where: path
      match: %^.*$%
    styles
    - develop_style
        file: frontend/develop._style
        media: all
- frontend_diagram_filter
    display
      check: url
      where: path
      match: %^/develop/structures/.*$%
    styles
    - form_style
        file: /system/module_page/frontend/form._style
        media: all
forms
- palette|form
    attributes
    - id: palette
    - method: post
    - novalidate: true
    children
    - palette|fieldset
        children
        - color|field_color
            title: Color for generation
            element_attributes
            - value: #7f7f7f
        - prefix|field_id_text
            title: Prefix
            element_attributes
            - name: prefix
            - value: custom_gray_
        - group|field_id_text
            title: Group
            element_attributes
            - name: group
            - value: custom_gray
        - multiplier_l|field_number
            title: Multiplier (left)
            element_attributes
            - name: multiplier_l
            - min: +1
            - max: +26
            - step: 1
            - value: 11
        - multiplier_r|field_number
            title: Multiplier (right)
            element_attributes
            - name: multiplier_r
            - min: +1
            - max: +26
            - step: 1
            - value: 11
        - report|fieldset
            title: Report
            children
            - palette|node
            - data|markup
                tag_name: x-document
                attributes
                - data-type: report
                children
                - data|text
                    text: The report will be created after submitting the form.
    - button_generate|button
        title: generate
        attributes
        - value: generate
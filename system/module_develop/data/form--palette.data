forms
- palette|form
    attributes
    - id: palette
    - method: post
    - novalidate: true
    children
    - palette|fieldset
        children
        - color|field_color
            title: Color for generation
            element_attributes
            - value: #f7f7f7
        - prefix|field_id_text
            title: Prefix
            element_attributes
            - name: prefix
            - value: custom_
        - group|field_id_text
            title: Group
            element_attributes
            - name: group
            - value: custom
        - report|fieldset
            title: Report
            children
            - palette|node
            - data|markup
                tag_name: x-document
                attributes
                - data-type: report
    - button_generate|button
        title: generate
        attributes
        - value: generate
frontend
- page_all_develop|frontend
    display
      check: url
      where: path
      match: %^/develop/.*$%
    styles
    - develop_style
        path: frontend/develop.cssd?id_page=%%_id_page_context
        attributes
        - rel: stylesheet
        - media: all
- page_structures_develop|frontend
    display
      check: url
      where: path
      match: %^/develop/structures/.*$%
    styles
    - form_style
        path: /system/module_page/frontend/form.cssd
        attributes
        - rel: stylesheet
        - media: all
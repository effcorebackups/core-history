frontend
- develop_page_all
    display
      check: url
      where: path
      match: %^.*$%
    styles
    - develop_style
        file: frontend/develop.cssd
        attributes
        - media: all
- develop_page_structures
    display
      check: url
      where: path
      match: %^/develop/structures/.*$%
    styles
    - form_style
        file: /system/module_page/frontend/form.cssd
        attributes
        - media: all
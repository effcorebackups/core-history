frontend
- develop_page_all|frontend
    display
      check: url
      where: path
      match: %^.*$%
    styles
    - develop_style
        file: frontend/develop.cssd
        attributes
        - rel: stylesheet
        - media: all
- page_structures|frontend
    display
      check: url
      where: path
      match: %^/develop/structures/.*$%
    styles
    - form_style
        file: /system/module_page/frontend/form.cssd
        attributes
        - rel: stylesheet
        - media: all
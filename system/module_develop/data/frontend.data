frontend
- page_all__develop|frontend
    display
      check: url
      where: path
      match: %^/develop/.*$%
    styles
    - page_style
        path: frontend/develop.cssd?page_id=%%_page_id_context
        attributes
        - rel: stylesheet
        - media: all
- page_structures__develop|frontend
    display
      check: url
      where: path
      match: %^/develop/structures/.*$%
    styles
    - form_style
        path: /system/module_page/frontend/form.cssd
        attributes
        - rel: stylesheet
        - media: all
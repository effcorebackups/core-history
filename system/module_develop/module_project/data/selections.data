- release_current|selection
    id: release_current
    title|text
      is_apply_translation: true
      is_apply_tokens: true
      text: Current release of the project %%_project_title_context
    decorator_settings
    - view_type: table-dl
    query_settings
    - conditions
      - id_project_!f: ~release.id_project
      - id_project_operator: =
      - id_project_!v|text
          is_apply_translation: false
          is_apply_tokens: true
          text: %%_project_id_context
    - order
      - build_!f: ~release.build
      - type: DESC
    - limit: 1
    fields
    - release.build
        type: field
        entity_name: release
        entity_field_name: build
    - release.path
        type: field
        entity_name: release
        entity_field_name: path
        is_visible: false
    - release_url
        type: markup
        title: File
        markup|markup
          tag_name: a
          attributes
          - href|text
              is_apply_translation: false
              is_apply_tokens: true
              text: http://%%_request_host/%%_selection_release_path_cur_context
          children
          - data|text
              text: Download
    - release.hash_sum
        type: field
        entity_name: release
        entity_field_name: hash_sum
    - release.created
        type: field
        entity_name: release
        entity_field_name: created
    - release.updated
        type: field
        entity_name: release
        entity_field_name: updated
    - release.description
        type: field
        entity_name: release
        entity_field_name: description
        settings
        - filters
          - 400: translate

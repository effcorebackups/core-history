tests
- register_user|test
    id: register_user
    scenario
    - form_id_set|step
        type: set
        name: form_id
        value: registration
    - validation_id_set|step
        type: set
        name: validation_id
        value: %%_validation_id
    - email_set|step
        type: set
        name: email
        value: test@example.com
    - nick_set|step
        type: set
        name: nick
        value: test
    - password_set|step
        type: set
        name: password
        value: 12345
    - session_params_remember_set|step
        type: set
        name: session_params[]
        value: remember
    - session_params_fixed_ip_set|step
        type: set
        name: session_params[]
        value: fixed_ip
    - captcha_set|step
        type: set
        name: captcha
        value: %%_captcha
    - button_set|step
        type: set
        name: button
        value: register
    - form_submit|step
        type: request
        url: /user/registration
        https: false
    - response_check|step
        type: check
        where: response
        match: 200
        on_success
        - return_result|step
            type: return
            value: true
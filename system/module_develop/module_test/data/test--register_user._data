tests
- register_user|test
    id: register_user
    scenario
    - init_captcha|step
        type: request
        url: /user/resister
        https: false
    - values_set|step
        type: set
        values
        - form_id: registration
        - validation_id: %%_validation_id
        - email: test@example.com
        - nick: test
        - password: 12345
        - session_params[0]: remember
        - session_params[1]: fixed_ip
        - captcha: xxxxxx
        - button: register
    - form_submit|step
        type: request
        url: /user/registration
        https: false
    - response_check|step
        type: check
        where: http_code
        match: 302
        on_success
        - return_result|step
            type: return
            value: true
tests
- load|test
    id: load
    title: Load
    scenario
    - repeat|step_repeat
        quantity: 10
        actions
        - request_page_login|step_request
            url: /user/login
        - response_check|step_check
            where: http_code
            match: 200
            on_failure
            - return_result|step_return
                value: 0
    - return_result|step_return
        value: 1
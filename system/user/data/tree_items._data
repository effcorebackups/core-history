tree_items
- login|tree_item
    id: login
    id_parent: user_anonymous
    parent_is_tree: true
    title: login
    weight: -20
    access
      roles
      - anonymous: anonymous
    attributes
    - href: /user/login
    - class
      - login: login
- registration|tree_item

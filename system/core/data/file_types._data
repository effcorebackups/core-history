file_types
# ─────────────────────────────────────────────────────────────────────
# protected
# ─────────────────────────────────────────────────────────────────────
- php
    type: php
    protected: 1
- sh
    type: sh
    protected: 1
- _mark
    type: _mark
    protected: 1
- _template
    type: _template
    protected: 1
# ─────────────────────────────────────────────────────────────────────
# with tokens
# ─────────────────────────────────────────────────────────────────────
- _style
    type: _style
    use_tokens: 1
    headers
    - Content-type: text/css
- _vector
    type: _vector
    use_tokens: 1
    headers
    - Content-type: image/svg+xml
- _script
    type: _script
    use_tokens: 1
    headers
    - Content-type: text/javascript
# ─────────────────────────────────────────────────────────────────────
# other types
# ─────────────────────────────────────────────────────────────────────
- css
    type: css
    headers
    - Content-type: text/css
- js
    type: js
    headers
    - Content-type: text/javascript
- html
    type: html
    headers
    - Content-type: text/html
- xml
    type: xml
    headers
    - Content-type: text/xml
- txt
    type: txt
    headers
    - Content-type: text/plain
- md
    type: md
    headers
    - Content-type: text/markdown
- image
    type: image
    headers
    - Content-type: image/*
- svg
    type: svg
    headers
    - Content-type: image/svg+xml
- ico
    type: ico
    headers
    - Content-type: image/x-icon
- gif
    type: gif
    headers
    - Content-type: image/gif
- jpg
    type: jpg
    headers
    - Content-type: image/jpeg
- png
    type: png
    headers
    - Content-type: image/png
file_types
# ─────────────────────────────────────────────────────────────────────
# protected
# ─────────────────────────────────────────────────────────────────────
- php
    type: php
    protected: 1
- sh
    type: sh
    protected: 1
- _mark
    type: _mark
    protected: 1
- _template
    type: _template
    protected: 1
# ─────────────────────────────────────────────────────────────────────
# with tokens
# ─────────────────────────────────────────────────────────────────────
- _style
    type: _style
    use_tokens: 1
    headers
    - Content-type: text/css
- _vector
    type: _vector
    use_tokens: 1
    headers
    - Content-type: image/svg+xml
- _script
    type: _script
    use_tokens: 1
    headers
    - Content-type: text/javascript

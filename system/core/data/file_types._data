file_types
# ─────────────────────────────────────────────────────────────────────
# protected
# ─────────────────────────────────────────────────────────────────────
- code_php
    type: php
    protected: 1
- shell_script
    type: sh
    protected: 1
- markdown_protected
    type: _mark
    protected: 1
- template
    type: _template
    protected: 1
# ─────────────────────────────────────────────────────────────────────
# with tokens
# ─────────────────────────────────────────────────────────────────────
- styles_dynamic
    type: _style
    use_tokens: 1
    headers
    - Content-type: text/css
- image_svg_dynamic
    type: _vector
    use_tokens: 1
    headers
    - Content-type: image/svg+xml
- javascript_dynamic
    type: _script
    use_tokens: 1
    headers
    - Content-type: text/javascript
# ─────────────────────────────────────────────────────────────────────
# other types
# ─────────────────────────────────────────────────────────────────────
- styles
    type: css
    headers
    - Content-type: text/css
- javascript
    type: js
    headers
    - Content-type: text/javascript
- markup_html
    type: html
    headers
    - Content-type: text/html
- markup_xml
    type: xml
    headers
    - Content-type: text/xml
- text
    type: txt
    headers
    - Content-type: text/plain
- markdown
    type: md
    headers
    - Content-type: text/markdown
- image_generic
    type: image
    headers
    - Content-type: image/*
- image_svg
    type: svg
    headers
    - Content-type: image/svg+xml
- image_ico
    type: ico
    headers
    - Content-type: image/x-icon
- image_gif
    type: gif
    headers
    - Content-type: image/gif
- image_jpg
    type: jpg
    headers
    - Content-type: image/jpeg
- image_png
    type: png
    headers
    - Content-type: image/png
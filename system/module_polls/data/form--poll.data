forms
- poll|form
    title: Leave a vote
    title_is_visible: false
    attributes
    - id: poll
    - method: post
    - novalidate: true
    children
    - fields|fieldset
        title: Question
    - button_vote|button
        title: vote
        attributes
        - value: vote
    - button_cancel|button
        title: cancel
        attributes
        - value: cancel
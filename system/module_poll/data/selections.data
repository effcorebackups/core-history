    query_settings
    - limit: 50
    fields
    - main
      - id
          entity_field_name: id
          weight: 400
      - question
          entity_field_name: question
          is_apply_translation: true
          weight: 380
      - expired
          entity_field_name: expired
          weight: 360
      - created
          entity_field_name: created
          weight: 340

forms
- instance_insert|form
    attributes
    - id: instance_insert
    - enctype: multipart/form-data
    - method: post
    - novalidate: true
    children
    - button_default|button_default
        attributes
        - value: insert
    - fields|fieldset
    - button_insert|button
        title: insert
        attributes
        - value: insert
    - button_cancel|button
        title: cancel
        break_on_validate: true
        attributes
        - value: cancel
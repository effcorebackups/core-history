pages
- instance_redirect_to_select|page
    https: true
    display
      check: url
      where: path
      match: %^(?<base>/manage/instances)/(?<action>select)$%
    access
      roles
      - =: admins
    children
    - block_instance_redirect_to_select|page_part
        region: content
        type: code
        source: \effcore\manage_instances::redirect_to_select
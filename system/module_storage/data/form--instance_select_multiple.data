forms
- instance_select_multiple|form
    attributes
    - id: instance_select_multiple
    - enctype: multipart/form-data
    - method: post
    - novalidate: true
    children
    - button_insert|button
        title: 
        novalidate: true
        attributes
        - title|text
            text: Insert a new item of type "%%_entity_title_context" on new page.
            is_apply_tokens: true
        - data-style: narrow-insert
        - value: insert
    - actions|field_select
        title: Actions
        element_attributes
        - name: actions
        values
        - not_selected: - select -
        - delete: Delete
    - data|node
    - button_apply|button
        title: apply
        attributes
        - value: apply
forms
- instance_select_multiple|Form
    managing_group_id: null
    entity_name: null
    category_id: null
    is_show_result_message: true
    _selection: null
    _selected_instances: null
    attributes
    - id: instance_select_multiple
    - enctype: multipart/form-data
    - method: post
    - novalidate: true
    children
    - button_insert|Button
        title: null
        break_on_request_value_set: true
        break_on_validate: true
        attributes
        - title|Text
            is_apply_tokens: true
            text: Insert a new item of type "%%_entity_title_context" on new page.
        - data-style: insert
        - value: insert
        - disabled: true
    - actions|Field_Select
        title: Actions
        element_attributes
        - name: actions
        - disabled: true
        items
        - not_selected: - select -
        - delete: Delete
        disabled
        - =: delete
    - data|Node
    - button_apply|Button
        title: apply
        attributes
        - value: apply
        - disabled: true
        weight: -500
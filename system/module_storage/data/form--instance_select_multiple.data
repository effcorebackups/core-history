forms
- instance_select_multiple|form
    attributes
    - id: instance_select_multiple
    - enctype: multipart/form-data
    - method: post
    - novalidate: true
    children
    - button_add_new|button
        title: add
        novalidate: true
        attributes
        - value: add_new
    - actions|field_select
        title: Actions
        element_attributes
        - name: actions
        - disabled: true
        values
        - not_selected: - select -
        - delete: Delete
    - data|node
    - button_apply|button
        title: apply
        attributes
        - value: apply
        - disabled: true
entities
- selection|entity
    name: selection
    title: Selection
    title_plural: Selections
    catalog_name: selections
    managing_is_enabled: true
    has_button_insert_and_update: true
    has_message_for_additional_controls: true
    has_parallel_checking: true
    ws_is_embedded: true
    ws_module_id: true
    ws_data: true
    access_select
      roles
      - =: admins
      permissions
      - =: manage_data__content_page_selection__select
    access_insert
      roles
      - =: admins
      permissions
      - =: manage_data__content_page_selection__insert
    access_update
      roles
      - =: admins
      permissions
      - =: manage_data__content_page_selection__update
    access_delete
      roles
      - =: admins
      permissions
      - =: manage_data__content_page_selection__delete
    fields
    - id
        title: ID
        type: varchar
        size: 255
        collate: nocase
        not_null: true
        check: (id <> '')
        managing_is_enabled_on_insert: true
        managing_is_enabled_on_update: true
        managing_control_class: \effcore\field_id_text
        managing_control_properties
        - is_validate_uniqueness: true
        - weight: +400
        managing_control_element_attributes_on_update
        - disabled: true
    - title
        title: Title
        type: varchar
        size: 1024
        collate: nocase
        not_null: true
        check: (title <> '')
        filter_insert: \effcore\core::return_htmlspecialchars_encoded
        filter_update: \effcore\core::return_htmlspecialchars_encoded
        managing_is_enabled_on_insert: true
        managing_is_enabled_on_update: true
        managing_control_class: \effcore\field_text
        managing_control_properties
        - weight: +380
        managing_control_element_attributes
        - maxlength: 1024
    - attributes
        title: Attributes
        type: blob
        filter_select: unserialize
        filter_insert: \effcore\core::data_serialize
        filter_update: \effcore\core::data_serialize
        managing_is_enabled_on_insert: true
        managing_is_enabled_on_update: true
        managing_control_class: \effcore\widget_attributes
        managing_control_properties
        - name_complex: attributes
        - weight: +360
    - fields
        title: Fields
        type: blob
        filter_select: unserialize
        filter_insert: \effcore\core::data_serialize
        filter_update: \effcore\core::data_serialize
        managing_is_enabled_on_update: true
        managing_control_class: \effcore\widget_entity_fields
        managing_control_properties
        - name_complex: fields
        - weight: +340
    - query_settings
        title: Query settings
        type: blob
        filter_select: unserialize
        filter_insert: \effcore\core::data_serialize
        filter_update: \effcore\core::data_serialize
        managing_control_value_manual_get: true
        managing_control_value_manual_set: true
        managing_control_properties
        - weight: +320
    - decorator_settings
        title: Decorator settings
        type: blob
        filter_select: unserialize
        filter_insert: \effcore\core::data_serialize
        filter_update: \effcore\core::data_serialize
        managing_control_value_manual_get: true
        managing_control_value_manual_set: true
        managing_control_properties
        - weight: +300
    - created
        title: Created
        type: datetime
        default: null
        check: (created <> '')
        managing_control_class: \effcore\field_datetime_local
        managing_control_properties
        - weight: +280
    - updated
        title: Updated
        type: datetime
        default: null
        check: (updated <> '')
        managing_control_class: \effcore\field_datetime_local
        managing_control_properties
        - weight: +260
    constraints
    - primary_id
        type: primary
        fields
        - =: id
    indexes
    - index_created
        type: index
        fields
        - =: created
    - index_updated
        type: index
        fields
        - =: updated
frontend
- page_all|frontend
    display
      check: url
      where: path
      match: %^.*$%
    styles
    - page_style
        file: frontend/page.cssd
        attributes
        - rel: stylesheet
        - media: all
    scripts
    - page_variables
        file: frontend/core.jsd
        weight: +50
        attributes
        - defer: true
    - page_script
        file: frontend/page.js
        weight: +30
        attributes
        - defer: true
- form_all|frontend
    display
      check: block
      where: dpath
      match: %^forms/.*$%
    styles
    - form_style
        file: frontend/form.cssd
        attributes
        - rel: stylesheet
        - media: all
    scripts
    - form_script
        file: frontend/form.js
        weight: +10
        attributes
        - defer: true
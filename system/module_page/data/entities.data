    name: text
    title: Text
    title_plural: Texts
    catalog_name: texts
    managing_is_on: true
    managing_group: Content
    ws_created: true
    ws_updated: true
    fields
    - id
        title: ID
        type: varchar
        size: 255
        collate: nocase

forms
- seo_meta|form
    attributes
    - id: seo_meta
    - method: post
    - novalidate: true
    children
    - settings|fieldset
        title: Settings
        children
        - content|field_textarea
            title: Content in the &lt;head&gt;
            element_attributes
            - name: content
            - required: false
            - minlength: null
    - button_save|button
        title: save
        attributes
        - value: save
        - disabled: true
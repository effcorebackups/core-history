- role|entity
    name: role
    storage_id: main
    catalog_id: roles
    ws_created: true
    ws_updated: true
    title: Role
    fields
      id
        type: varchar
        size: 255
        not_null: true
      title
        type: varchar
        size: 255
        not_null: true
      is_embed
        type: integer
        not_null: true
        default: 0
    constraints
    - key_id
        type: primary key

- relation_role_ws_permission|entity
    name: relation_role_ws_permission
    title: Relation: Role + Permission
    title_plural: Relations: Role + Permission
    storage_id: main
    catalog_id: relations_role_ws_permission
    ws_created: true
    ws_updated: true
    fields
    - id_role
        title: Role ID
        type: varchar
        size: 255
        not_null: true
    - id_permission
        title: Permission ID
        type: varchar
        size: 255
        not_null: true
    constraints
    - primary_id_role_id_permission
        type: primary
        fields
        - =: id_role
        - =: id_permission
    - foreign_id_role
        type: foreign
        fields
        - =: id_role
        references: roles
        references_fields
        - =: id
    - foreign_id_permission
        type: foreign
        fields
        - =: id_permission
        references: permissions
        references_fields
        - =: id
- relation_role_ws_user|entity

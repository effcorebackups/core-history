- user|entity
    name: user
    title: User
    title_plural: Users
    storage_id: main
    catalog_id: users
    ws_created: true
    ws_updated: true
    fields
    - nick
        title: Nick
        type: varchar
        size: 32
        not_null: true
    - email
        title: EMail
        type: varchar
        size: 64
        not_null: true
    - password_hash
        title: Password hash
        hidden: true
        type: varchar
        size: 255
    - is_embed
        title: Is embed
        type: integer
        not_null: true
        default: 0
    - avatar_path_relative
        title: Avatar relative path
        hidden: true
        type: varchar
        size: 1024
    constraints
    - primary_nick
        type: primary
        fields
        - =: nick
    - unique_email
        type: unique
        fields
        - =: email
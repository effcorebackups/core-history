- relation_role_ws_user|entity
    name: relation_role_ws_user
    storage_id: main
    catalog_id: relations_role_ws_user
    ws_created: true
    ws_updated: true
    title: Relation: Role + User
    fields
      id_role
        type: varchar
        size: 255
        not_null: true

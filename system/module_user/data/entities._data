        title: Role ID
        type: varchar
        size: 255
        not_null: true
    - nick
        title: Nick
        type: varchar
        size: 32
        not_null: true
    constraints
    - primary_id_role_nick
        type: primary
        fields
        - =: id_role
        - =: nick
    - foreign_id_role
        type: foreign
        fields
        - =: id_role
        references: roles
        references_fields
        - =: id
    - foreign_nick
        type: foreign
        fields
        - =: nick
        references: users
        references_fields
        - =: nick

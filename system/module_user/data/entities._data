entities
- permission|entity
    name: permission
    title: Permission
    title_plural: Permissions
    storage_id: main
    catalog_id: permissions
    ws_created: true
    ws_updated: true
    fields
    - id
        title: ID
        type: varchar
        size: 255
        not_null: true
    - title
        title: Title
        type: varchar
        size: 255
        not_null: true
    constraints
    - primary_id
        type: primary
        fields

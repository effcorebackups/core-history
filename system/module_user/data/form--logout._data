forms
- logout|form
    attributes
    - id: logout
    - method: post
    - =: novalidate
    children
    - button_logout|markup
        tag_name: button
        attributes
        - type: submit
        - name: button
        - value: logout
        children
        - label|text
            text: logout
    - button_cancel|markup
        tag_name: button
        novalidate: true
        attributes
        - type: submit
        - name: button
        - value: cancel
        children
        - label|text
            text: cancel
forms
- feedback|form
    attributes
    - id: feedback
    - method: post
    - novalidate: true
    children
    - fields|fieldset
        title: Feedback
        children
        - email|field_email
        - message|field_textarea
            title: Message
            element_attributes
            - name: message
            - maxlength: 2048
        - captcha|pluggable_class
            name: field_captcha
    - button_send|button
        title: send
        attributes
        - value: send
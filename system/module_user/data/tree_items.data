tree_items
- login|tree_item
    id: login
    id_parent: M:user_anonymous
    title: Login
    url: /login
    weight: 100
    access
      roles
      - =: anonymous
    attributes
    - class
      - =: login
- registration|tree_item
    id: registration
    id_parent: M:user_anonymous
    title: Registration
    url: /registration
    access
      roles
      - =: anonymous
    attributes
    - class
      - =: registration
- recovery|tree_item
    id: recovery
    id_parent: M:user_anonymous
    title: Recovery
    url: /recovery
    access
      roles
      - =: anonymous
    attributes
    - class
      - =: recovery
# ─────────────────────────────────────────────────────────────────────
- user_edit|tree_item
    id: user_edit
    id_parent: M:user_registered
    title: Edit
    url: /user/%%_nick/edit
    access
      roles
      - =: admins
      - =: registered
    attributes
    - class
      - =: user-edit
- logout|tree_item
    id: logout
    id_parent: M:user_registered
    title: Logout
    url: /logout
    weight: -100
    access
      roles
      - =: registered
    attributes
    - class
      - =: logout
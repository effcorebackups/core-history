- user|entity
    name: user
    title: User
    title_plural: Users
    catalog_name: users
    group: User
    ws_created: true
    ws_updated: true
    fields
    - id
        title: ID
        type: autoincrement
        not_null: true
        show_in_manager: true
        field_class: \effcore\field_number
        field_element_attributes
        - min: 1
        - disabled: true
    - is_embed
        title: Is embed
        type: boolean
        not_null: true
        default: 0
        show_in_manager: true
        field_class: \effcore\field_logic
        field_element_attributes
        - disabled: true
    - nick
        title: Nick
        type: varchar
        size: 32
        collate: nocase
        not_null: true
        show_in_manager: true
        field_class: \effcore\field_nick
    - email
        title: EMail
        type: varchar
        size: 64
        collate: nocase
        not_null: true
        show_in_manager: true
        field_class: \effcore\field_email
    - password_hash
        title: Password
        type: varchar
        size: 255
        collate: binary
        field_class: \effcore\field_password
    - avatar_path
        title: Avatar path
        type: varchar
        size: 1024
        collate: nocase
    - timezone
        title: Timezone
        type: varchar
        size: 64
        collate: nocase
        default: UTC
        field_class: \effcore\field_timezone
    constraints
    - primary_id
        type: primary
        fields
        - =: id
    - unique_nick
        type: unique
        fields
        - =: nick
    - unique_email
        type: unique
        fields
        - =: email
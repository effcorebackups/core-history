- session|entity
    name: session
    title: Session
    title_plural: Sessions
    catalog_name: sessions
    group: User
    fields
    - id
        title: ID
        type: varchar
        size: 255
        collate: nocase
        not_null: true
        field_is_visible_on_select: true
        field_class: \effcore\field_text
        field_element_attributes
        - pattern: ^[a-zA-Z0-9_\-]+$
        - disabled: true
    - id_user
        title: User ID
        type: integer
        not_null: true
        field_is_visible_on_select: true
        field_class: \effcore\field_relation

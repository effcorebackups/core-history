- user|entity
    name: user
    title: User
    title_plural: Users
    catalog_name: users
    ws_created: true
    ws_updated: true
    fields
    - id
        title: ID
        type: autoincrement
        not_null: true
    - nick
        title: Nick
        type: varchar
        size: 32
        collate: nocase
        not_null: true
    - email
        title: EMail
        type: varchar
        size: 64
        collate: nocase
        not_null: true
    - password_hash
        title: Password hash
        hidden: true
        type: varchar
        size: 255
        collate: binary
    - is_embed
        title: Is embed
        type: boolean
        not_null: true
        default: 0
    - avatar_path
        title: Avatar path
        hidden: true
        type: varchar
        size: 1024
        collate: nocase
    - timezone
        title: Timezone
        type: varchar
        size: 64
        collate: nocase
        default: UTC
    constraints
    - primary_id
        type: primary
        fields
        - =: id
    - unique_nick
        type: unique
        fields
        - =: nick
    - unique_email
        type: unique
        fields
        - =: email
selections
- instance_select-feedback|selection
    id: instance_select-feedback
    decorator_settings
    - view_type: table-dl
    query_settings
    - conditions
      - id_!f: ~feedback.id
      - id_operator: =
      - id_!v|text
          is_apply_translation: false
          is_apply_tokens: true
          text: %%_selection__feedback__id_context
    fields
    - feedback.id
        type: field
        entity_name: feedback
        entity_field_name: id
        weight: 400
    - feedback.name
        type: field
        entity_name: feedback
        entity_field_name: name
        weight: 380
    - feedback.email
        type: field
        entity_name: feedback
        entity_field_name: email
        weight: 360
    - feedback.message
        type: field
        entity_name: feedback
        entity_field_name: message
        weight: 340
    - feedback.created
        type: field
        entity_name: feedback
        entity_field_name: created
        weight: 320
    - feedback.updated
        type: field
        entity_name: feedback
        entity_field_name: updated
        weight: 300

forms
- login|form
    attributes
    - id: login
    - method: post
    - novalidate: true
    children
    - credentials|fieldset
        children
        - email|field_email
        - password|field_password
        - session_params|group_switchers
            element_attributes
            - name: session_params[]
            values
            - is_long_session: long session
            - is_fixed_ip: bind to my IP address
            checked
            - =: is_long_session
            - =: is_fixed_ip
        - captcha|form_plugin
            name: field_captcha
    - button_login|button
        title: login
        attributes
        - value: login
        weight: -500
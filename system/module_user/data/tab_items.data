tab_items
- data_user|tab_item
    id: data_user
    id_parent: null
    id_tab: data
    action_name: user
    action_name_default: user/user
    title: Users
    weight: +30
    access
      roles
      - =: admins
      permissions_match
      - =: %^manage_data__user_.+$%
- data_user_role|tab_item
    id: data_user_role
    id_parent: data_user_user
    id_tab: data
    action_name: user/role
    title: Roles
    access
      roles
      - =: admins
      permissions
      - =: manage_data__user_role__select

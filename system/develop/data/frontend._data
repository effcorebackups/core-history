frontend
- frontend_page_n
    display
      check: path
      where: url
      match: %.*%
    styles
    - develop_style
        file: frontend/develop._style
        media: all
- frontend_diagram_filter
    display
      check: path
      where: url
      match: %/develop/structures/.*%
    styles
    - form_style
        file: /page/frontend/form._style
        media: all
entities
- captcha|entity
    name: captcha
    storage_id: main
    fields
      id
        type: autoincrement
        not_null: true
      characters
        type: varchar
        size: 32
        not_null: true
      canvas_data
        type: blob
        not_null: true
      attempts
        type: integer
        not_null: true
    constraints
    - primary
        type: primary key
        fields
        - id: id
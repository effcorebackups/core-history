forms
- form_test|form
    attributes
      id: form_test
      novalidate: novalidate
      method: post
      class: form-test
    children
    - fieldset_main|markup
        tag_name: fieldset
        children
        - fieldset_main_legend|markup
            tag_name: legend
            children
            - content|text
                text: Element &lt;legend&gt;&lt;/legend&gt;
      # field_input_text
        - field_input_text_label|markup
            tag_name: label
            children
            - content|text
                text: Element &lt;input type="text"&gt;
        - field_input_text|markup
            tag_name: input
            title: Input (type=text)
            attributes
              type: text
              name: field_input_text
              value|text
                text: text in input
              required: required
              minlength: 5
              maxlength: 255
      # field_input_password
        - field_input_password_label|markup
            tag_name: label
            children
            - content|text
                text: Element &lt;input type="password"&gt;

entities
- user|entity
    name: user
    storage_id: db
    fields
      id
        auto: true
        type: integer
        not_null: true
      email
        type: varchar
        size: 255
        not_null: true
      login
        type: varchar
        size: 255
        not_null: true
      password_hash
        type: varchar
        size: 255
      created
        type: timestamp
        not_null: true
        default: current_timestamp
      is_embed
        type: integer
        not_null: true
        default: 0
    indexes
    - primary
        type: primary key
        fields
        - id: id
    - unique_email
        type: unique key
        fields
        - email: email
    - unique_login
        type: unique key
        fields
        - login: login

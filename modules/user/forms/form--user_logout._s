forms
- form_user_logout|form
    attributes
    - id: user_logout
    - novalidate: novalidate
    - method: post
    - class
      - default: form-user
    children
    - button_logout|markup
        tag_name: button
        attributes
        - type: submit
        - name: button
        - value: logout
        children
        - label|text
            text: logout
    - button_cancel|markup
        tag_name: button
        novalidate: true
        attributes
        - type: submit
        - name: button
        - value: cancel
        children
        - label|text
            text: cancel
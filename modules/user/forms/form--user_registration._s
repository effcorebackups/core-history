forms
- form_user_registration|form
    attributes
    - id: user_registration
    - novalidate: novalidate
    - method: post
    - class
      - default: form-user
    children
    - fieldset_default|markup
        tag_name: fieldset
        children
        - field_email|form_field
            tag_name: input
            title: Email
            attributes
            - type: email
            - name: email
            - required: required
            - minlength: 5
            - maxlength: 255
        - field_password|form_field
            tag_name: input
            title: Password
            attributes
            - type: password
            - name: password
            - required: required
            - minlength: 5
            - maxlength: 255
            - autocomplete: off
    - button_register|markup
        tag_name: button
        attributes
        - type: submit
        - name: button
        - value: register
        children
        - label|text
            text: register
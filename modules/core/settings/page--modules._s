pages
- page_modules|page
    title: Modules
    match
      url: %^/admin/modules$%
    access
      roles
      - admins: admins
    content
    - block_tree_main
        region: nav
        type: link
        npath: trees/core/main
    - block_tree_user
        region: nav
        type: link
        npath: trees/user/user
    - block_admin_modules
        region: content_1_1
        type: code
        handler: \effectivecore\events_page::on_show_admin_modules
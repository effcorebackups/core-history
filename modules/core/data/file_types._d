file_types
- code_php
    type: php
    protected: 1
    headers
    - Content-type: text/html
- styles
    type: css
    use_tokens: 1
    headers
    - Content-type: text/css
- javascript
    type: js
    use_tokens: 1
    headers
    - Content-type: text/javascript
- markup_html
    type: html
    use_tokens: 1
    headers
    - Content-type: text/html
- markup_xml
    type: xml
    use_tokens: 1
    headers
    - Content-type: text/xml
- text
    type: txt
    headers
    - Content-type: text/plain
- image_generic
    type: image
    headers
    - Content-type: image/*
- image_svg
    type: svg
    use_tokens: 1
    headers
    - Content-type: image/svg+xml
- image_ico
    type: ico
    headers
    - Content-type: image/x-icon
- image_gif
    type: gif
    headers
    - Content-type: image/gif
- image_jpg
    type: jpg
    headers
    - Content-type: image/jpeg
- image_png
    type: png
    headers
    - Content-type: image/png
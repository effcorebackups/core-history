file_types
# protected files
- php
    protected: 1
    headers
    - Content-type: text/html
# text files
- css
    use_tokens: 1
    headers
    - Content-type: text/css
- js
    use_tokens: 1
    headers
    - Content-type: text/javascript
- html
    use_tokens: 1
    headers
    - Content-type: text/html
- xml
    use_tokens: 1
    headers
    - Content-type: text/xml
- txt
    headers
    - Content-type: text/plain
# images
- image
    headers
    - Content-type: image/*
- svg
    use_tokens: 1
    headers
    - Content-type: image/svg+xml
- ico
    headers
    - Content-type: image/x-icon
- gif
    headers
    - Content-type: image/gif
- jpg
    headers
    - Content-type: image/jpeg
- png
    headers
    - Content-type: image/png
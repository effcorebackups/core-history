entities
- tree|entity
    name: tree
    storage_id: main
    fields
      id
        type: autoincrement
        not_null: true
      title
        type: varchar
        size: 255
        not_null: true
    constraints
    - key_id
        type: primary key
        fields
        - id: id
- tree_item|entity
    name: tree_item
    storage_id: main
    fields
      id
        type: autoincrement
        not_null: true
      id_parent
        type: integer
        not_null: true
      parent_is_tree
        type: integer
        not_null: true
        default: 0
      title
        type: varchar
        size: 255
        not_null: true
    constraints
    - key_id
        type: primary key
        fields
        - id: id